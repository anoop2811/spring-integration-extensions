<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:int="http://www.springframework.org/schema/integration"
	xmlns:task="http://www.springframework.org/schema/task" xmlns:util="http://www.springframework.org/schema/util"
	xmlns:int-apns="http://www.springframework.org/schema/integration/apns"
	xsi:schemaLocation="http://www.springframework.org/schema/integration/apns http://www.springframework.org/schema/integration/spring-integration-apns/spring-integration-apns.xsd
		http://www.springframework.org/schema/integration http://www.springframework.org/schema/integration/spring-integration.xsd
		http://www.springframework.org/schema/task http://www.springframework.org/schema/task/spring-task.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">
	<int:channel id="test-input" />

	<int-apns:outbound-channel-adapter
		certificate-path="classpath:src/test/resources/mock-cert.p12"
		key-store-password="clairmail" apns-host="localhost" apns-port="5155" channel="test-input">
	</int-apns:outbound-channel-adapter>

	<int-apns:feedback-channel-adapter
		id="test" apns-host="localhost" apns-port="5153" certificate-path="classpath:src/test/resources/mock-cert.p12"
		key-store-password="clairmail" channel="output-channel">
		<int:poller cron="*/10 * * * * *"></int:poller>
	</int-apns:feedback-channel-adapter>

	<int:channel id="output-channel" />

	<int:logging-channel-adapter channel="output-channel"
		level="ERROR" log-full-message="true" id="testLog" />



</beans>

